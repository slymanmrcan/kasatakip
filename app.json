{
  "expo": {
    "name": "kasatakip",
    "slug": "kasatakip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kasatakip",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.CAMERA"],
      "package": "com.smtechlab.kasatakip"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "react-native-vision-camera",
        {
          "enableFrameProcessors": true,
          "cameraPermission": "KasaTakip, fişleri ve etiketleri taramak için kameranızı kullanmak istiyor."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "visionCamera": {
        "enableFrameProcessors": true
      },
      "router": {},
      "eas": {
        "projectId": "b78ff1d5-ceaf-4fd2-8b98-c06693d4a2c8"
      }
    }
  }
}
